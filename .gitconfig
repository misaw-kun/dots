[init]
	defaultBranch = main

[core]
	editor = vim

[color]
	ui = true

[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green

[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold

[color "status"]
	added = yellow
	changed = green
	untracked = red
        
[web]
	browser = firefox

[credential]
	helper = cache --timeout 21600 # six hours
	helper = oauth
